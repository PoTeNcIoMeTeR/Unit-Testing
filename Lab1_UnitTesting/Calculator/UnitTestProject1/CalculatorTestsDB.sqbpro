DROP TABLE IF EXISTS FormatTestCases;

-- Створюємо її заново
CREATE TABLE FormatTestCases (
    Id INTEGER PRIMARY KEY AUTOINCREMENT,
    TestName TEXT NOT NULL,
    InputExpression TEXT NOT NULL,
    ExpectedResult TEXT NOT NULL
);

-- Наповнюємо її даними
INSERT INTO FormatTestCases (TestName, InputExpression, ExpectedResult) VALUES
--  Успішні стандартні сценарії
('Valid simple', '2+2', '2+2'),
('Valid with spaces', ' 5 * (10 - 2) ', '5*(10-2)'),
('Empty expression', '', ''),
('Valid expression with brackets', '(5-3)*2', '(5-3)*2'),

-- Тести для СПЕЦИФІЧНОЇ логіки унарних операторів ('m'/'p') 
('Valid unary ''m'' at start', 'm5+10', 'm5+10'),
('Valid unary ''p'' at start', 'p5-3', 'p5-3'),
('Valid unary ''m'' in brackets', '5*(m2)', '5*(m2)'),
('Valid unary ''p'' in brackets', '3+(p5)', '3+(p5)'),

-- Сценарії, що ПРОВАЛЮЮТЬСЯ через некоректну логіку коду 
('FAIL: Standard unary minus start', '-5+10', '&amp;Error 03 — Невірна синтаксична конструкція вхідного виразу.'),
('FAIL: Standard unary minus in brackets', '5*(-2)', '&amp;Error 03 — Невірна синтаксична конструкція вхідного виразу.'),

--  Сценарії стандартних помилок 
('ERROR 01: Digit before bracket', '5(2+2)', '&amp;Error 01 at &lt;1&gt; — Неправильна структура в дужках, помилка на &lt;1&gt; символі.'),
('ERROR 02: Unknown symbol', '5 # 2', '&amp;Error 02 at &lt;1&gt; — Невідомий оператор на &lt;1&gt; символі.'),
('ERROR 03: Invalid start with operator', '*5-2', '&amp;Error 03 — Невірна синтаксична конструкція вхідного виразу.'),
('ERROR 03: Operator after open bracket', '(+5)', '&amp;Error 03 — Невірна синтаксична конструкція вхідного виразу.'),
('ERROR 04: Two operators', '5++2', '&amp;Error 04 at &lt;2&gt; — Два підряд оператори на &lt;2&gt; символі.'),
('ERROR 05: Invalid end with operator', '5+2-', '&amp;Error 05 — Незавершений вираз.');
</sql><current_tab id="0"/></tab_sql></sqlb_project>
